<rxui:ReactiveWindow xmlns="https://github.com/avaloniaui"
                     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                     xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                     xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                     xmlns:vm="using:FileDialogMvvm.ViewModels"
                     xmlns:rxui="http://reactiveui.net"
                     mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
                     x:Class="FileDialogMvvm.Views.DialogWindow"
                     x:TypeArguments="vm:DialogViewModel"
                     Title="DialogWindow">
  <StackPanel>
    <TextBox Text="{Binding Text}"/>
    <Button Command="{Binding Ok}">OK</Button>
  </StackPanel>
</rxui:ReactiveWindow>
